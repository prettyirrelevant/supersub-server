{
  "name": "supersub-server",
  "main": "src/index.ts",
  "scripts": {
    "dev": "tsx watch --clear-screen=false src/index.ts",
    "format": "eslint . --ext .ts,.js,.mts --fix",
    "test:cov": "vitest run --coverage",
    "lint": "eslint . --ext .ts,.js,.mts",
    "clean": "rimraf dist coverage",
    "build": "rimraf dist && tsup",
    "db:dev": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:status": "prisma migrate status",
    "db:generate": "prisma generate",
    "start": "cross-env ENVIRONMENT=production node dist/index.js",
    "test:dev": "vitest dev",
    "test": "vitest run"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/bun": "latest",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-perfectionist": "^2.7.0",
    "eslint-plugin-prettier": "^5.1.3",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.7",
    "supertest": "^7.0.0",
    "tsup": "^8.0.2",
    "tsx": "^4.10.5",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@alchemy/aa-accounts": "^3.16.0",
    "@alchemy/aa-core": "^3.16.0",
    "@bull-board/api": "^5.17.1",
    "@bull-board/express": "^5.17.1",
    "@prisma/client": "^5.14.0",
    "@privy-io/server-auth": "^1.7.3",
    "alchemy-sdk": "^3.3.1",
    "bullmq": "^5.7.10",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.11",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "jose": "^5.3.0",
    "pm2": "^5.3.1",
    "prisma": "^5.14.0",
    "resend": "^3.2.0",
    "viem": "^2.12.1",
    "winston": "^3.13.0",
    "zod": "^3.23.8",
    "zod-validation-error": "^3.3.0"
  }
}
